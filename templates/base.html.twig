<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Met√©o des services{% endblock %}</title>
    <link rel="icon" href="/images/meteo-sun-cloud.svg">

    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}

    <script>
        window.prolongation_ENT_args = {
            current: "{{ ent_name }}",
            delegateAuth: true
        };
    </script>
    <script src="{{ prolongation_ent_js }}"></script>
</head>
<body>
{% for type, messages in app.flashes() %}
    {% for message in messages %}
    <div class="alert alert-{{type == 'error' ? 'danger' : 'success'}} superpose ml-auto">
        {{ message }}
    </div>
    {% endfor %}
{% endfor %}

    <div class="sidebar"></div>
    <div class="marge" id="main-block">
        <div class="container mt-5">
            <div class="row mt-3">
            </div>

{% block body %}
{% endblock %}
</body>
</html>
